<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="plugin example" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug DLL_Win32">
				<Option output="..\bin\plugins\standard_devices-d" prefix_auto="1" extension_auto="1" />
				<Option object_output="..\..\z_output_temp\pluma\examples\debug" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-g" />
					<Add option="-O0" />
				</Compiler>
				<Linker>
					<Add option="-lpluma-d" />
				</Linker>
			</Target>
			<Target title="Release DLL_Win32">
				<Option output="..\bin\plugins\standard_devices" prefix_auto="1" extension_auto="1" />
				<Option object_output="..\..\z_output_temp\pluma\examples\release" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-O3" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-lpluma" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-DPLUGIN_EXPORTS" />
			<Add directory="..\..\include" />
			<Add directory="..\" />
		</Compiler>
		<Linker>
			<Add directory="..\..\bin" />
		</Linker>
		<Unit filename="..\plugins\Keyboard.hpp" />
		<Unit filename="..\plugins\Mouse.hpp" />
		<Unit filename="..\plugins\StandardDevices.cpp" />
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
